{
    "name": "fabiang/msgfmt",
    "description": "Gettext MO file parser and PO generator",
    "license": "BSD-2-Clause",
    "homepage": "https://github.com/fabiang/msgfmt",
    "keywords": ["gettext", "msgmft"],
    "type": "library",
    "authors": [
        {
            "name": "Fabian Grutschus",
            "email": "f.grutschus@lubyte.de"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Fabiang\\Msgfmt\\": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "ext-gettext": "*",
        "phpunit/phpunit": "~4.4",
        "zendframework/zend-i18n": "~2.2",
        "symfony/translation": "~2.6",
        "satooshi/php-coveralls": "0.6.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
